<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProjetTest</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProjetTest.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
          <a id="Acceuil"  asp-page="Index">Acceuil</a>   
            <a id="Propos"  asp-page="/propos"> A Propos</a>
          <a id="Propos" asp-area="" asp-controller="" asp-action="Aide"> Aide</a>
         
          <div id="log">  <partial name="_LoginPartial" /> </div> 
                           
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>   



    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
   
        </main>
   
    </div>

     <footer class="border-top footer text-muted">
   
   <center>   <div class="container">
            &copy; Design by <i>Ariel Basso</i> <a asp-area="" asp-controller="Home" asp-action="Privacy"></a>
        </div> </center>  
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>